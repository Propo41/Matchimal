<%- include("partials/header_no_nav"); -%>
    <div id="spinner_id" class="loading" style="display: none;"></div>
    <div class="section1">
        <div class="container">
            <h2>LOG IN</h2>
            <form action="/" id="form" class="form login-input">
                <input type="email" id="email" name="email" placeholder="Email" required>
                <input type="password" id="password" name="password" placeholder="Password" required>
                <input id="loginButton" onclick="login()" type="button" value="Log in">
            </form>
            <a class="nav-link active forgotPassword" href="#">Forgot Password?</a>
            <h5>or log in with</h5>
            <div style="text-align:center">
                <button type="button" class="btn btn-outline-dark">Google</button>
            </div>
        </div>
    </div>

    <div style="height: 100px;"> </div>

    <%- include("partials/footer_no_nav"); -%>